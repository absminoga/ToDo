.section_two
    .container
        .wrapper
            .container_section_menu
                .container_section_menu_top
                    .container_date
                    .container_time
                .container_section_menu_buttom
                    ul.container_tabs
                        li.section_two_tabs.js-tab-trigger.active
                            i.fas.fa-plus-circle(data-tab="1")
                            span add task
                        li.section_two_tabs.js-tab-trigger
                            i.fas.fa-calendar-alt(data-tab="2")
                            span toDo task
                        li.section_two_tabs.js-tab-trigger
                            i.fas.fa-calendar-check(data-tab="3")
                            span completed task
                        li.section_two_tabs.js-tab-trigger
                            i.fas.fa-calendar-times(data-tab="4")
                            span expired task
            .container_section_task_dashboard
                .container_section_task_dashboard_header
                    h2 toDo list
                .tab_content
                    .tabs_dashboard.js-tab-content.active(data-tab="1")
                        div.tabs_dashboard_list_card 
                            h3 add new task
                            label.tabs_dashboard_list_card_title(for="new_title")
                                h4 title
                            textarea(type="text")#new_title
                            p.text-counter 
                                span used&nbsp;
                                span.char-counter 0&nbsp;
                                span / 30
                            label.tabs_dashboard_list_card_description(for="new_task")
                                h4 description task
                            textarea(type="text")#new_task
                            buttom.add_btn 
                                i.fas.fa-folder-plus
                    .tabs_dashboard.js-tab-content(data-tab="2")
                        .container_todo.container_item
                            h3 todo task
                            ul.incompleted_tasks 
                                li
                                    input(type="checkbox")
                                    .block_text_description
                                        textarea(type="text" disabled).input_title
                                        textarea(type="text"  disabled).input_description
                                    .button_item
                                        button.edit edit
                                        button.delete delete
                    .tabs_dashboard.js-tab-content(data-tab="3")
                        .container_completed.container_item
                            h3 completed task
                            ul.completed_tasks
                                li.changes
                                    input(type="checkbox")
                                    .block_text_description
                                        textarea(type="text"  disabled).input_title  Пример
                                        textarea(type="text"  disabled).input_description Пример
                                    .button_item
                                        button.edit edit
                                        button.delete delete
                    .tabs_dashboard.js-tab-content(data-tab="4")
                        .container_expired.container_item
                            h3 expired task
                            ul.expired_tasks
                                li.expired
                                    input(type="checkbox")
                                    .block_text_description
                                        textarea(type="text" disabled).input_title  Пример
                                        textarea(type="text" disabled).input_description  Пример
                                    .button_item
                                        button.edit edit
                                        button.delete delete



                
                    